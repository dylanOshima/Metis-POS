{
  "name": "Metis-client",
  "author": "dylanOshima",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "mousetrap": "^1.6.3",
    "react-autosuggest": "^9.4.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.2.0",
    "redux-thunk": "^2.3.0",
    "react": "^16.8.6",
    "react-redux": "^5.1.1",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "jest": "^24.9.0",
    "react-scripts": "1.0.17",
    "redux-mock-store": "^1.5.3"
  },
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "DEVELOPMENT=true react-scripts start",
    "build": "react-scripts build && npm run build-electron",
    "test-react": "react-scripts test --env=jsdom",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "eject": "react-scripts eject",
    "electron-start": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "cp -r electron/. build/electron && cp -r src/shared/. build/src/shared",
    "package": "electron-builder build --mac --win -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "metis-pos",
      "owner": "dylanoshima"
    }
  }
}
